{
    "app" :{
        "port" : 5000,
        "debug": "*"
    },

    "storage" : {
        "type" : "FileStore" 
    },

    "temp" : "/tmp/",

    "redis":{
        "host": "localhost",
        "port": 6379,
        "return_buffers": true
    },

    "rediskey":{
        "filestore" : "filestore",
        "ttl" : 30
    },

    "webshot":{
        "siteType"   : "url",
        "renderDelay":2000,
        "screenSize":{
            "height":480,
            "width":320
        },
        "shotSize"   : {
            "width"  : "window",
            "height" : "window"
        },
        "defaultWhiteBackground": true,
        "streamType" : "jpg"
    }
}
