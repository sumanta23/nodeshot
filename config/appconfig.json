{
    "app" :{
        "url":"https://nodeshot.herokuapp.com",
        "port" : 5000,
        "cluster": true,
        "nworker" : 5,
        "debug": "*,-express*,-body-parser*"
    },

    "auth" : {
        "enable" : false
    },

    "storage" : {
        "type" : "FileStore" 
    },

    "temp" : "/tmp/",

    "redis":{
        "host": "localhost",
        "port": 6379,
        "return_buffers": true
    },

    "rediskey":{
        "filestore" : "filestore",
        "ttl" : 30
    }
}
